<div class="board-tiles-messages">
    <form id="game-board"  >
        {{#each boardSquares}}
            <div class="game-board-tile l-{{this.letter_multiplier}}-w-{{this.word_multiplier}}"
            data-x="{{this.x}}" data-y="{{this.y}}" id="square({{this.x}},{{this.y}})"
            ondrop="drop(event)" ondragover="allowDrop(event)">

                <p>  </p>

            </div>
        {{/each}}
    </form>
    {{> tileRack}}
    {{> chatMessagesGameBoard messageArray=messages ready=isReady players=players}}

    
</div>


